{
  "version": 2,
  "builds": [
    {
      "src": "src/pages/index.html",
      "use": "@vercel/public/"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/styles",
      "destination": "public/styles/"
    },
    {
      "source": "/javascript",
      "destination": "public/javascript/"
    },
    {
      "source": "/images",
      "destination": "public/images/"
    },
    {
      "source": "/icons",
      "destination": "public/icons/"
    }
  ]
}
