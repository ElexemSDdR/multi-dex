{
  "version": 2,
  "builds": [
    {
      "src": "src/pages/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/public",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/pages/$1"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/public/styles/$1"
    },
    {
      "src": "/javascript/(.*)",
      "dest": "/public/javascript/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/public/icons/$1"
    }
  ]
}
